<odoo>
  <data>
    <record id="res_partner_form_inherit_route" model="ir.ui.view">
            <field name="name">res.partner.form.inherit.olila.route</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='vat']" position="after">
                    <field name="area_id" />
                    <field name="territory_id" />
                    <field name="route_id" />
                    <field name="division" />
<!--                    <field name="district" />-->

                </xpath>
            </field>
        </record>
  </data>
</odoo>